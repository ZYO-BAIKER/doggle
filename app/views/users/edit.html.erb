<%= render "shared/header"%>

<div class='form-wrap'>
  <%= form_with model: current_user, class: 'registration-user', local: true do |f| %>
  <%= render 'shared/error_messages', model: f.object %>

    <h2 class='title'><%= @nickname %> さんのプロフィール編集</h2>
    <%= render partial:'devise/shared/registration', locals: { f: f } %>

    <div class='register-btn'>
      <%= f.submit "更新する" ,class:"reg-btn" %>
    </div>

  <% end %>
</div>