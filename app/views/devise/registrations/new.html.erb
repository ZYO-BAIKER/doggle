<%= render "shared/header"%>

<div class='form-wrap'>
  <%= form_with model: @user, url: user_registration_path, class: 'registration-user', local: true do |f| %>
  <%= devise_error_messages! %>

  <%= render partial:'devise/shared/registration', locals: { f: f } %>

    <div class='register-btn'>
      <%= f.submit "会員登録" ,class:"reg-btn" %>
    </div>

  <% end %>
</div>